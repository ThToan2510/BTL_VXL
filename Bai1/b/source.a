		ORG 0000H
		CALL MAIN
		ORG 2000H
MAIN:	MOV R0, #0F0H
NEW:	MOV P1, R0
		MOV R2, #200
LOOP1:	ACALL OUTPUT
		DJNZ R2, LOOP1
		MOV A, R0
		RR A
		MOV R0,A
		SJMP NEW
		RET
OUTPUT:	MOV P1, R0
		MOV R6,#4
		MOV A, R0
		ACALL DELAY
		ACALL DELAY
LOOP:	ACALL DELAY
		RR A
		ORL A, R0
		MOV P1, A
		DJNZ R6, LOOP
		RET
DELAY:	MOV R4, #100
		DJNZ R4, $
		RET
		END

