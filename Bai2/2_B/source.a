			ORG 0000H
MAIN:		MOV R0, #0
			MOV DPTR, #DATA1
CORE:		MOV A,R0
			MOVC A, @A + DPTR
			MOV P1, A
			CALL DELAY
			INC R0
			CJNE R0, #0AH, CORE
			SJMP MAIN
DATA1: DB 0FCH, 60H, 0DAH, 0F2H, 66H, 0B6H, 0BEH, 0E0H, 0FEH, 0F6H
DELAY:		MOV R2, #8
LOOP2:		MOV R3, #240
LOOP1:		MOV R4, #240
			DJNZ R4, $
			DJNZ R3, LOOP1
			DJNZ R2, LOOP2
			RET
			END
				 